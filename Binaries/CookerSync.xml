    <FileGroup Tag="Content" Platform="*">
      <Files>
        <FileSet Path="%GAME%\Cooked%PLATFORM%%CODERMODE%\*.*" bIsRecursive="true" >
          <FileFilters>
            <FileFilter Name="*_%LANGUAGE%.xxx" />
            <FileFilter Name="*_%LANGUAGE%.upk" />
            <FileFilter Name="*_%LANGUAGE%.bin" />
            <FileFilter Name="*_%LANGUAGE%_full.bin" />
            <FileFilter Name="*_%NOTLANGUAGE%.xxx" />
            <FileFilter Name="*_%NOTLANGUAGE%.upk" />
            <FileFilter Name="*_%NOTLANGUAGE%.bin" />
            <FileFilter Name="*_%NOTLANGUAGE%_full.bin" />
