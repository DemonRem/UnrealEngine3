/** The entry point. */
void Main(
	in float4 InPosition : POSITION,
	in float2 InUV : TEXCOORD0,
	out float4 OutPosition : POSITION,
	out float2 OutUV : TEXCOORD0
	)
{
	OutPosition = InPosition;
	OutUV = InUV;
}
