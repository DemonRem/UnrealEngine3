#include "Common.usf"

void Main(
	float2 InPosition : POSITION,
	float2 InUV       : TEXCOORD0,
	out float4 OutPosition : POSITION,
	out float2 OutUV       : TEXCOORD0
	)
{
	OutPosition = float4( InPosition, 0, 1 );
    OutUV = InUV;
}
